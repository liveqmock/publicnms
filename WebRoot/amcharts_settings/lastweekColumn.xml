<settings>
  <data_type>xml</data_type>
  <background>
    <alpha>100</alpha>
    <border_alpha>0</border_alpha>
  </background>
  <plot_area>
    <color>000000</color>
    <border_color></border_color>                                                                 
    <border_alpha></border_alpha>                                                                   
    <margins>                                                 
      <left>40</left>                                         
      <top>40</top>                                           
      <right>20</right>                                       
      <bottom>55</bottom>     
    </margins>
  </plot_area>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <axes>
    <category>
      <tick_length>10</tick_length>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <depth>10</depth>
  <column>
  	<type>clustered</type>      
     <width>70</width>                                        
    <spacing>10</spacing>            
    <alpha>85</alpha>      
    <balloon_text>{value}</balloon_text>
    <grow_time>3</grow_time>
  </column>
   <legend>                                                    
    <enabled>false</enabled>                                   
  </legend> 
  <labels>
    <label lid="0">
      <text><![CDATA[<b>近一周频率较高的告警</b>]]></text>
      <y>5</y>
      <text_color>000000</text_color>
      <text_size>14</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>